            }
        }
    );
    $(function () {
                    $('#vat_token_check').on('click', function (e) {
            e.preventDefault();
            $('.vat_entry_overlay').removeClass('d-none')
            $("#after_check_token_toggle").addClass('d-none');
            $("#res_vat_details").addClass('d-none');
            $('.vat_entry_btn').prop('disabled', true)
            document.getElementById('check_icon').className = '';
            let vat_token = $('#vat_token').val();
            $.ajax({
                url: "https://marketmaster-bimotion.org/ajax/check-vat-token",
                type: "POST",
                data: {
                    "_token": "ZEIMPngdBU6WbAVLAg4rMjwJlaM0wtm9IGFOyxoC",
                    vat_token: vat_token
                },
                beforeSend: function () {
                    $("#check_icon").addClass('fas fa-spinner fa-spin');
                },
                success: function (response) {
                    console.log(response)
                    if (response.status === "success") {
                        $('#vat_token').removeClass('is-invalid');
                        $("#check_icon").removeClass('fas fa-spinner fa-spin');
                        $("#after_check_token_toggle").removeClass('d-none');
                        $("#res_vat_details").removeClass('d-none');
                        $('#res_vat_token').text(response.vat_token)
                        $('#res_vat_amount').text(response.amount)
                        $('#res_target_user_model').text(response.user_model)
                        $('#res_payment_by').text(response.payment_by)
                        $('#res_payment_status').text(response.payment_status)
                        $('#vat_entry_id').val(response.id)
                        $('.vat_entry_overlay').addClass('d-none')
                        $('.vat_entry_btn').prop('disabled', false)
                    }
                    if (response.status === "error") {
                        $("#after_check_token_toggle").addClass('d-none');
                        $("#res_vat_details").addClass('d-none');
                        $('#vat_token').addClass('is-invalid');
                        $("#check_icon").removeClass('fas fa-spinner fa-spin');
                        $('.vat_entry_overlay').addClass('d-none')
                        
                        

                    }
                },
                error: function (response) {
                    $('.vat_entry_overlay').addClass('d-none')
                    $("#check_icon").removeClass('fas fa-spinner fa-spin');
                    
                    
                },
            });
        });
    })
</script>


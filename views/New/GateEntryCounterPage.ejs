<%- include('partials/header') %>
              <div class="card w-100">
                <div class="card-body">
                  <div class="col-lg-12">
                    <div class="row d-flex align-items-center justify-content-between">
                      <div class="col-md-8">
                        <h4 class="card-title">Purchase Entry Form</h4>
                      </div>
                        <form class="forms-sample">
                          <div class="form-group">
                            <div class="input-group">
                              <input type="text" class="form-control" placeholder="Enter Customer Code" aria-label="Customer Check">
                              <div class="input-group-append">
                                <button class="btn btn-sm btn-primary" type="button">Check</button>
                              </div>
                              <div class="ml-2">
                                <button class="btn btn-md btn-primary" style="border-radius: 0;" type="button" data-toggle="modal" data-target="#addCustomerModal">
                                  <i class="ti-user"></i> ADD
                                </button>
                              </div>
                            </div>
                            <div class="mt-3">
                              <label>Members</label>
                              <div class="input-group">
                                <select class="form-control">
                                  <option>Select Member</option>
                                  <option>Member1</option>
                                  <option>Member2</option>
                                  <option>Member3</option>
                                </select>
                              </div>
                            </div>
                          </div>  
                            <!-- Modal Structure -->
                            <div class="modal fade" id="addCustomerModal" tabindex="-1" role="dialog" aria-labelledby="addCustomerModalLabel" aria-hidden="true">
                              <div class="modal-dialog" role="document">
                                <div class="modal-content">
                                  <div class="modal-header">
                                    <h2 class="modal-title" id="addCustomerModal"><u>Create Customer</u></h2>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                      <span aria-hidden="true">&times;</span>
                                    </button>
                                  </div>
                                  <div class="modal-body">
                                    <!-- Include your user registration form here -->
                                    <iframe src="../AddCustomerModal.ejs" width="100%" height="800px" style="border:none;"></iframe>
                                  </div>
                                </div>
                              </div>
                            </div>
                            <button type="button" class="btn btn-primary btn-block">Confirm Gate Entry</button>                       
                        </form>
                    </div>
                  </div>
                </div>
              </div>

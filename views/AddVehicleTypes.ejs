<%- include('partials/header') %>
              <div class="card w-100">
                <div class="card-body">
                  <div class="col-lg-12">
                    <div class="row d-flex align-items-center justify-content-between">
                      <div class="col-md-8">
                        <h4 class="card-title">Add Vehicle Type Form</h4>
                        <p class="card-description">
                          Add a new Type of Vehicle
                        </p>
                        <form class="forms-sample">
                          <div class="form-group">
                            <label for="exampleInputUsername1">Vehicle ID</label>
                            <input type="text" class="form-control" id="exampleInputUsername1" placeholder="Vehicle ID Number">
                          </div>
                          <div class="form-group">
                            <label for="exampleInputCity1">Vehicle Name</label>
                            <input type="text" class="form-control" id="exampleInputCity1" placeholder="Vehicle Name">
                          </div>
                          <button type="submit" class="btn btn-primary mr-2">Submit</button>
                          <button class="btn btn-light">Cancel</button>
                        </form>
                  </div>
                </div>
              </div>
            </div>
            
<%- include('partials/footer') %>
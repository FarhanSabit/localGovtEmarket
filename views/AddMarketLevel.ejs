<%- include('partials/header') %>

      <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title">Add Market Level Form</h4>
            <p class="card-description">
              Add a new Market Level
            </p>
            <form class="forms-sample">
              <div class="form-group">
                <label for="exampleInputUsername1">Market Level</label>
                <select class="form-control form-control-lg" id="exampleLevelSelect1">
                  <option>L1</option>
                  <option>L2</option>
                  <option>L3</option>
                  <option>L4</option>
                  <option>L5</option>
                  <option>L6</option>
                </select>
              </div>
              <div class="form-group">
                <label for="exampleInputCity1">Shop name</label>
                <select class="form-control form-control-lg" id="exampleShopSelect1">
                  <option>A1</option>
                  <option>A2</option>
                  <option>B1</option>
                  <option>B2</option>
                  <option>C1</option>
                  <option>C2</option>
                </select>
              </div>
              <div class="form-group">
                <label for="exampleFormControlSelect1">Status</label>
                <select class="form-control form-control-lg" id="exampleFormControlSelect1">
                  <option>Active</option>
                  <option>Inactive</option>
                </select>
              </div>
              <button type="submit" class="btn btn-primary">Submit</button>
              <button class="btn btn-light">Cancel</button>
            </form>
          </div>
        </div>
      </div>
      <%- include('partials/footer') %>
